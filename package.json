{
  "name": "clean-node",
  "version": "1.4.1",
  "main": "index.js",
  "repository": "https://github.com/leonfoliveira/clean-node.git",
  "author": "Leonardo Oliveira <leonfoliveira@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "yarn build && concurrently -k 'yarn build:watch' 'yarn debug'",
    "debug": "node-dev --respawn --no-notify --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --no-cache --runInBand --silent",
    "test:unit": "yarn test --testMatch '**/*.spec.ts'",
    "test:integration": "yarn test --testMatch '**/*.test.ts'",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "update": "yarn-check -u -s"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "celebrate": "^13.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "14.14.21",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "26.0.1",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mockdate": "^3.0.2",
    "node-dev": "^6.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "6.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "yarn-check": "^0.0.3"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
